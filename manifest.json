{
	"name": "GRLC Browser",
	"description": "Search the Baked Blocks of Garlic on ChromeOS",
	"version": "0.1",
	"manifest_version": 2,
	"app": {
		"background": {
			"scripts": ["background.js"]
		}
	},
	"incognito": "not_allowed",
